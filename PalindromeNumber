class Solution {
    public boolean isPalindrome(int x) {
        int rev=0,t,p;
        p=x;
        while(x>0)
        {
            t = x%10;
            rev = rev*10+t;
            x=x/10;
        }
        if(rev == p)
        {
            return true;
        }
        else
        {
            return false;
        }
        
    }
}
